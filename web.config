<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <add value="login.asp" />
            </files>
        </defaultDocument>
        <rewrite>
            <rules>
                <rule name="ReverseProxyInboundRule1" stopProcessing="true">
                    <match url="(node.asp)" />
                    <action type="Rewrite" url="http://localhost:3000/node/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
